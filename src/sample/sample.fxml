<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="1.0" vgap="1.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />

   </columnConstraints>
   <rowConstraints>
      <RowConstraints />

   </rowConstraints>
   <children>
      <Pane fx:id="paneConnect" prefHeight="277.0" prefWidth="288.0">
         <children>
            <TextField fx:id="txtServerIP" layoutX="67.0" layoutY="15.0" prefHeight="25.0" prefWidth="204.0" text="localhost" />
            <TextField fx:id="txtNickname" layoutX="67.0" layoutY="48.0" prefHeight="25.0" prefWidth="204.0" text="123" />
            <Button fx:id="cmdConnect" layoutX="199.0" layoutY="83.0" mnemonicParsing="false" onAction="#cmdConnectPressed" text="Connect" />
            <Label layoutX="13.0" layoutY="20.0" text="Server:" />
            <Label layoutX="12.0" layoutY="53.0" text="Nick:" />
         </children>
      </Pane>
      <Pane fx:id="paneMain" prefHeight="209.0" prefWidth="200.0" visible="false">
         <children>
            <TextField fx:id="txtCallTo" editable="false" layoutX="72.0" layoutY="102.0" prefHeight="25.0" prefWidth="136.0" />
            <Button fx:id="cmdCall" layoutX="221.0" layoutY="102.0" mnemonicParsing="false" onAction="#cmdCallPressed" prefHeight="25.0" prefWidth="47.0" text="Call" />
            <Label layoutX="16.0" layoutY="106.0" text="Call to:" />
            <Label layoutX="16.0" layoutY="139.0" text="Status:" />
            <Label fx:id="lblStatus" layoutX="72.0" layoutY="138.0" prefHeight="17.0" prefWidth="204.0" text="Nothing interesting" />
            <Button fx:id="cmdFinishCall" layoutX="202.0" layoutY="164.0" mnemonicParsing="false" onAction="#cmdFinishCallPressed" prefHeight="25.0" prefWidth="72.0" text="Finish" visible="false" />
            <ListView fx:id="lbUsersOnline" layoutX="17.0" layoutY="24.0" prefHeight="71.0" prefWidth="257.0" />
            <TextArea fx:id="txtLog" editable="false" layoutX="17.0" layoutY="197.0" prefHeight="63.0" prefWidth="257.0" wrapText="true" />
         </children>
      </Pane>
   </children>
</GridPane>
