<GridPane alignment="center" hgap="1.0" vgap="1.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainController">
   <columnConstraints>
      <ColumnConstraints />

   </columnConstraints>
   <rowConstraints>
      <RowConstraints />

   </rowConstraints>
   <children>
      <Pane fx:id="paneMain" prefHeight="480.0" prefWidth="800.0">
         <children>
            <TextField fx:id="txtCallTo" editable="false" layoutX="14.0" layoutY="429.0" prefHeight="25.0" prefWidth="117.0" visible="false" />
            <Button fx:id="cmdCall" disable="true" layoutX="155.0" layoutY="87.0" mnemonicParsing="false" onAction="#cmdCallPressed" prefHeight="25.0" prefWidth="47.0" text="Call" />
            <Label layoutX="592.0" layoutY="92.0" text="Status:" />
            <Label fx:id="lblStatus" layoutX="592.0" layoutY="115.0" prefHeight="17.0" prefWidth="204.0" text="Nothing interesting" />
            <Button fx:id="cmdFinishCall" layoutX="591.0" layoutY="139.0" mnemonicParsing="false" onAction="#cmdFinishCallPressed" prefHeight="25.0" prefWidth="72.0" text="Finish" visible="false" />
            <ListView fx:id="lbUsersOnline" layoutX="14.0" layoutY="124.0" prefHeight="338.0" prefWidth="190.0" />
            <TextArea fx:id="txtLog" editable="false" layoutX="592.0" layoutY="246.0" prefHeight="217.0" prefWidth="190.0" wrapText="true" />
            <TextFlow fx:id="tfUser" layoutX="14.0" layoutY="20.0" prefHeight="63.0" prefWidth="190.0" />
            <TextFlow fx:id="tfMe" layoutX="592.0" layoutY="14.0" prefHeight="63.0" prefWidth="190.0" />
            <Label layoutX="591.0" layoutY="225.0" text="History:" />
            <TextArea fx:id="txtMessageInput" layoutX="238.0" layoutY="402.0" prefHeight="55.0" prefWidth="271.0" promptText="Type message here" />
            <Button fx:id="cmdSendMessage" layoutX="510.0" layoutY="402.0" mnemonicParsing="false" onAction="#cmdSendMessagePressed" prefHeight="25.0" prefWidth="63.0" text="Send" />
            <WebView fx:id="wvMessageHistory" layoutX="238.0" layoutY="24.0" prefHeight="370.0" prefWidth="335.0" />
         </children>
      </Pane>
   </children>
</GridPane>
